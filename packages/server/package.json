{
  "name": "@agent-devtools/server",
  "version": "0.1.0",
  "description": "MCP server exposing React Native app debug data to AI agents",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "bin": {
    "agent-devtools-server": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "start": "bun run src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@agent-devtools/shared": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "ws": "^8.19.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.9"
  },
  "files": [
    "src",
    "dist"
  ]
}
